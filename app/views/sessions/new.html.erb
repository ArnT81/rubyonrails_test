<h1>Sign in</h1>

<%= form_with data: { turbo: false }, url: sign_in_path do |form| %>
	<div class="mb-3">
		<%= form.label :email %>
		<%= form.text_field :email, class: "form-control", placeholder: "example@mail.com" %>
	</div>

	<div class="mb-3">
		<%= form.label :password %>
		<%= form.password_field :password, class: "form-control", placeholder: "password" %>
	</div>

	<div>
		<%= form.submit "Log In", class: "btn btn-primary"%>
	</div>
<%end%>